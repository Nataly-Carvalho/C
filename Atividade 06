//Valores dos passos a seguir
#include <stdio.h>

main() {
int MEDIA, NOTA, TOTAL, X, J, A, I, K, Z;


MEDIA = 0;
NOTA = 2;
TOTAL = 10;
Z = 0;
K = 7;
X == 4;
I = 80;
J = -1;
A = -4;


X = TOTAL/NOTA;
printf("Valor de X = %d", X);

X = X+1;
printf("\nValor de x = %d", X);

NOTA = X;
printf("\nValor de NOTA = %d", NOTA);

TOTAL = NOTA + X + TOTAL;
printf("\nValor de TOTAL = %d", TOTAL);

MEDIA = 3;
printf("\nValor de MEDIA = %d", MEDIA);

K +=9;
printf("\nValor de K = %d", K);

Z = J/5;
printf("\nValor de Z = %d", Z);

K = I % 5;
printf("\nValor de K = %d", K);

I++;
printf("\nValor de I = %d", I);

I = I+2*A;
printf("\nValor de I = %d", I);

  
  return 0;
}
